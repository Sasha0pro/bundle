{
    "name": "sasha0pro/symfony_bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "symfony/console": "7.0.*",
        "symfony/yaml": "7.0.*"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DTOService\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTOService\\Tests\\": "tests/"
        }
    }
}